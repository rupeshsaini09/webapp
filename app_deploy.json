{
	"apiVersion": "apps/v1",
	"kind": "Deployment",
	"metadata": {
		"name": "rupesh-webapp"
	},
	"spec": {
		"selector": {
			"matchLabels": {
				"app": "frontend"
			}
		},
		"template": {
			"metadata": {
				"name": "abc",
				"labels": {
					"app": "frontend"
				}
			},
			"spec": {
				"containers": [
					{
						"name": "pqr",
						"image": "rupeshsaini09/rupesh-webapp:latest",
						"ports": [
							{
								"containerPort": 80
							}
						]
					}
				]
			}
		}
	}
}
